    <div class="container" style="margin-top: 60px;">
        <div class=" row pt-5 ">
            <div class="col-md-6 m-auto myOtpPage">
                <div class="container height-100 d-flex justify-content-center align-items-center">
                    <div class="position-relative">
                        <div class="card  text-center">

                            <form action="verifyOtp" method="post">
                                <h1 style="margin-top: 10px; margin-bottom: 20px;">Enter OTP</h1>
                                <h6>Please enter the one time password <br> to verify your account</h6>
                                <div id="otp" class="inputs d-flex flex-row justify-content-center mt-2">
                                    <input class="m-2 text-center form-control rounded  otpLen" maxlength="1"
                                        name="otp1" type="text" />
                                    <input class="m-2 text-center form-control rounded  otpLen" maxlength="1"
                                        name="otp2" type="text" />
                                    <input class="m-2 text-center form-control rounded  otpLen" maxlength="1"
                                        name="otp3" type="text" />
                                    <input class="m-2 text-center form-control rounded  otpLen" maxlength="1"
                                        name="otp4" type="text" />
                                    <input class="m-2 text-center form-control rounded  otpLen" maxlength="1"
                                        name="otp5" type="text" />
                                    <input class="m-2 text-center form-control rounded  otpLen" maxlength="1"
                                        name="otp6" type="text" />
                                </div>
                                <% var msg %>
                                <% var sendMsg %>
                                <% var otpSend %>
                                <% if (otpSend) { %>
                                    <div>
                                        <span class="text-success">
                                            <%= sendMsg %>
                                        </span>
                                    </div>
                                <% } %>
                                    <% if (msg) { %> 
                                        <div>
                                            <span class="text-danger">
                                                <%= msg %>
                                            </span>
                                        </div>
                                        <% } %>
                                            <div class="mt-4 pb-3">                                                

                                                <a href="">
                                                    <button type="submit"
                                                        class="btn btn-success px-4 validate">Verify</button>
                                                </a>
                                            </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
